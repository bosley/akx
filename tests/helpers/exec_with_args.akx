(begin
  (let args (os/args))
  (io/putf "Script received arguments:\n")
  (let idx 0)
  (loop (lt idx 5)
    (begin
      (if (eq idx 0)
        (io/putf "  [%d] %s\n" idx "tests/helpers/exec_with_args.akx")
        (begin))
      (if (eq idx 1)
        (io/putf "  [%d] %s\n" idx "hello")
        (begin))
      (if (eq idx 2)
        (io/putf "  [%d] %s\n" idx "world")
        (begin))
      (if (eq idx 3)
        (io/putf "  [%d] %s\n" idx "123")
        (begin))
      (if (eq idx 4)
        (io/putf "  [%d] %s\n" idx "456.780000")
        (begin))
      (set idx (+ idx 1))))
  42)

