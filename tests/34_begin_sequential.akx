(import "$AKX_HOME/stdlib/io.akx")
(import "$AKX_HOME/stdlib/math.akx")

(println "=== Testing begin ===")

(println "Begin returns last value:")
(let result (begin
  (+ 1 2)
  (+ 3 4)
  (+ 5 6)))
(println "  result:" result)

(println "Begin with side effects:")
(let x 10)
(begin
  (set x (+ x 5))
  (set x (* x 2))
  (println "  x after operations:" x))

(println "Empty begin returns nil:")
(let empty-result (begin))
(println "  empty-result:" empty-result)

(println "=== All begin tests passed ===")

