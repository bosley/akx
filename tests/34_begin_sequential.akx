(import "$AKX_HOME/stdlib/math.akx")

(io/putf "=== Testing begin ===\n")

(io/putf "Begin returns last value:\n")
(let result (begin
  (+ 1 2)
  (+ 3 4)
  (+ 5 6)))
(io/putf "  result: %d\n" result)

(io/putf "Begin with side effects:\n")
(let x 10)
(begin
  (set x (+ x 5))
  (set x (* x 2))
  (io/putf "  x after operations: %d\n" x))

(io/putf "Empty begin returns nil:\n")
(let empty-result (begin))
(io/putf "  empty-result: %v\n" empty-result)

(io/putf "=== All begin tests passed ===\n")

