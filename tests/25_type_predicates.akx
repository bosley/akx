(io/putf "=== Type Predicate Tests ===\n")

(let x 42)
(io/putf "(?int 42): %d\n" (?int x))
(io/putf "(?real 42): %d\n" (?real x))
(io/putf "(?str 42): %d\n" (?str x))

(let y 3.14)
(io/putf "(?real 3.14): %d\n" (?real y))
(io/putf "(?int 3.14): %d\n" (?int y))

(let s "hello")
(io/putf "(?str \"hello\"): %d\n" (?str s))
(io/putf "(?int \"hello\"): %d\n" (?int s))

(let f (lambda [x] x))
(io/putf "(?lambda f): %d\n" (?lambda f))
(io/putf "(?int f): %d\n" (?int f))

(io/putf "=== All type predicates passed ===\n")

