(io/putf "=== Numeric Comparison Tests ===\n")

(io/putf "--- Greater Than ---\n")
(io/putf "(gt 5 3): %d\n" (gt 5 3))
(io/putf "(gt 3 5): %d\n" (gt 3 5))
(io/putf "(gt 5 5): %d\n" (gt 5 5))

(io/putf "--- Greater Than or Equal ---\n")
(io/putf "(gte 5 3): %d\n" (gte 5 3))
(io/putf "(gte 3 5): %d\n" (gte 3 5))
(io/putf "(gte 5 5): %d\n" (gte 5 5))

(io/putf "--- Less Than ---\n")
(io/putf "(lt 3 5): %d\n" (lt 3 5))
(io/putf "(lt 5 3): %d\n" (lt 5 3))
(io/putf "(lt 5 5): %d\n" (lt 5 5))

(io/putf "--- Less Than or Equal ---\n")
(io/putf "(lte 3 5): %d\n" (lte 3 5))
(io/putf "(lte 5 3): %d\n" (lte 5 3))
(io/putf "(lte 5 5): %d\n" (lte 5 5))

(io/putf "--- Real Numbers ---\n")
(io/putf "(gt 3.5 2.1): %d\n" (gt 3.5 2.1))
(io/putf "(lt 2.1 3.5): %d\n" (lt 2.1 3.5))
(io/putf "(gte 3.5 3.5): %d\n" (gte 3.5 3.5))
(io/putf "(lte 3.5 3.5): %d\n" (lte 3.5 3.5))

(io/putf "=== Numeric comparison tests passed ===\n")

