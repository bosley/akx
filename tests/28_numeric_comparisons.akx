(import "$AKX_HOME/stdlib/io.akx")

(println "=== Numeric Comparison Tests ===")

(println "--- Greater Than ---")
(println "(gt 5 3):" (gt 5 3))
(println "(gt 3 5):" (gt 3 5))
(println "(gt 5 5):" (gt 5 5))

(println "--- Greater Than or Equal ---")
(println "(gte 5 3):" (gte 5 3))
(println "(gte 3 5):" (gte 3 5))
(println "(gte 5 5):" (gte 5 5))

(println "--- Less Than ---")
(println "(lt 3 5):" (lt 3 5))
(println "(lt 5 3):" (lt 5 3))
(println "(lt 5 5):" (lt 5 5))

(println "--- Less Than or Equal ---")
(println "(lte 3 5):" (lte 3 5))
(println "(lte 5 3):" (lte 5 3))
(println "(lte 5 5):" (lte 5 5))

(println "--- Real Numbers ---")
(println "(gt 3.5 2.1):" (gt 3.5 2.1))
(println "(lt 2.1 3.5):" (lt 2.1 3.5))
(println "(gte 3.5 3.5):" (gte 3.5 3.5))
(println "(lte 3.5 3.5):" (lte 3.5 3.5))

(println "=== Numeric comparison tests passed ===")

