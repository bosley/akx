(io/putf "=== Lambda Equality Tests ===\n")

(let f (lambda [x] x))
(let g (lambda [x] x))
(let h f)

(io/putf "(eq f f): %d\n" (eq f f))
(io/putf "(eq f h): %d\n" (eq f h))
(io/putf "(eq f g): %d\n" (eq f g))

(io/putf "(neq f g): %d\n" (neq f g))
(io/putf "(neq f h): %d\n" (neq f h))

(io/putf "=== Lambda equality tests passed ===\n")

