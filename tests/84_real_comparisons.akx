(io/putf "Test: Real number comparison operations\n\n")

(io/putf "Test 1: real/eq equality\n")
(let c1 (real/eq 5.5 5.5))
(io/putf "  5.5 == 5.5: %d\n" c1)
(let c2 (real/eq 5.5 5.6))
(io/putf "  5.5 == 5.6: %d\n" c2)
(let c3 (real/eq 3.0 3.0 3.0))
(io/putf "  3.0 == 3.0 == 3.0: %d\n" c3)

(io/putf "\nTest 2: real/neq inequality\n")
(let c4 (real/neq 5.5 6.5))
(io/putf "  5.5 != 6.5: %d\n" c4)
(let c5 (real/neq 5.5 5.5))
(io/putf "  5.5 != 5.5: %d\n" c5)
(let c6 (real/neq 1.0 2.0 3.0))
(io/putf "  1.0 != 2.0 != 3.0: %d\n" c6)

(io/putf "\nTest 3: real/gt greater than\n")
(let c7 (real/gt 10.5 5.5))
(io/putf "  10.5 > 5.5: %d\n" c7)
(let c8 (real/gt 5.5 10.5))
(io/putf "  5.5 > 10.5: %d\n" c8)
(let c9 (real/gt 5.5 5.5))
(io/putf "  5.5 > 5.5: %d\n" c9)

(io/putf "\nTest 4: real/gte greater than or equal\n")
(let c10 (real/gte 10.5 5.5))
(io/putf "  10.5 >= 5.5: %d\n" c10)
(let c11 (real/gte 5.5 5.5))
(io/putf "  5.5 >= 5.5: %d\n" c11)
(let c12 (real/gte 5.5 10.5))
(io/putf "  5.5 >= 10.5: %d\n" c12)

(io/putf "\nTest 5: real/lt less than\n")
(let c13 (real/lt 5.5 10.5))
(io/putf "  5.5 < 10.5: %d\n" c13)
(let c14 (real/lt 10.5 5.5))
(io/putf "  10.5 < 5.5: %d\n" c14)
(let c15 (real/lt 5.5 5.5))
(io/putf "  5.5 < 5.5: %d\n" c15)

(io/putf "\nTest 6: real/lte less than or equal\n")
(let c16 (real/lte 5.5 10.5))
(io/putf "  5.5 <= 10.5: %d\n" c16)
(let c17 (real/lte 5.5 5.5))
(io/putf "  5.5 <= 5.5: %d\n" c17)
(let c18 (real/lte 10.5 5.5))
(io/putf "  10.5 <= 5.5: %d\n" c18)

(io/putf "\nTest complete.\n")

