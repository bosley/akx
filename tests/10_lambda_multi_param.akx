(cjit-load-builtin println :root "tests/builtins/println.c")

(let first (lambda [a b c] a))
(let second (lambda [a b c] b))
(let third (lambda [a b c] c))

(let r1 (first 1 2 3))
(let r2 (second 1 2 3))
(let r3 (third 1 2 3))

(println "first(1,2,3) =" r1)
(println "second(1,2,3) =" r2)
(println "third(1,2,3) =" r3)

(assert-eq r1 1)
(assert-eq r2 2)
(assert-eq r3 3)
(println "PASS: multi-parameter lambdas work")
