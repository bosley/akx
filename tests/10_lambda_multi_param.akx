(let first (lambda [a b c] a))
(let second (lambda [a b c] b))
(let third (lambda [a b c] c))

(let r1 (first 1 2 3))
(let r2 (second 1 2 3))
(let r3 (third 1 2 3))

(io/putf "first(1,2,3) = %d\n" r1)
(io/putf "second(1,2,3) = %d\n" r2)
(io/putf "third(1,2,3) = %d\n" r3)

(assert/eq r1 1)
(assert/eq r2 2)
(assert/eq r3 3)
(io/putf "PASS: multi-parameter lambdas work\n")
