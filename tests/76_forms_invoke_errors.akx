(io/putf "=== Forms Invoke Error Handling Tests ===\n")

(io/putf "Test 1: Valid invocation baseline\n")
(form/define :Valid :int)
(form/add-affordance :Valid :inc (lambda [x] (+ x 1)))
(let val 10)
(let result (form/invoke val :inc))
(if (eq result 11)
  (io/putf "  PASS: Valid invocation works\n")
  (io/putf "  FAIL: Baseline should work\n"))

(io/putf "\n=== All Error Handling Tests Complete ===\n")
